// 导航显示隐藏
function naviToggle() {
	$(".container-navi").toggle(500, function(){
		var isdisplay = $(".container-navi").css("display");
		if (isdisplay == "none") {
			$(".container-body").css({"width":"100%"});
		} else if (isdisplay == "block") {
			$(".container-body").css({"width":"90%"});
		}
	});
}